{
  "name": "fight-club",
  "version": "1.0.0",
  "description": "Multiplayer competitive coding game",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "test-setup": "cd server && node test-setup.js",
    "seed": "curl -X POST http://localhost:5000/api/admin/seed"
  },
  "keywords": ["coding", "competitive", "multiplayer", "game"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
